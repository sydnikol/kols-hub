{
  "name": "kol-unified-mega-app",
  "version": "4.0.0",
  "description": "KOL - Your Self-Evolving Personal OS - Complete Unified Application",
  "type": "module",
  "private": true,
  "author": "Kol",
  "license": "MIT",
  "main": "electron/main.cjs",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "start": "npm run dev",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "android": "cap sync android && cap open android",
    "ios": "cap sync ios && cap open ios",
    "mobile:sync": "cap sync",
    "mobile:build": "npm run build && cap sync",
    "build:android": "npm run mobile:build && cap open android",
    "build:ios": "npm run mobile:build && cap open ios",
    "setup:mobile": "npm i @capacitor/core @capacitor/cli @capacitor/android @capacitor/ios && npx cap init && npx cap add android && npx cap add ios",
    "pwa:build": "vite build --mode production",
    "deploy": "npm run build && netlify deploy --prod",
    "update": "npm update && npm audit fix",
    "clean": "rm -rf node_modules package-lock.json dist && npm install",
    "server": "node server/index.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
